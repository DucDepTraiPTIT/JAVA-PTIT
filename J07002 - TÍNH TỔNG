package myjava1;
import java.util.*;
import static java.lang.Math.*;
import java.io.*;
public class Myjava1{
    public static void main(String[] args) throws FileNotFoundException{
        Scanner sc = new Scanner(new File("DATA.in"));
        long sum = 0;
        while(sc.hasNext()){
            String s = sc.next();
            try{
                sum += Integer.parseInt(s);
            } catch(Exception e){
                
            }
        }
        System.out.println(sum);
}
}
